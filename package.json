{
  "name": "elm-poker",
  "version": "1.0.0",
  "description": "A simple repository to show Elm integration with Firebase using ports",
  "main": "src/index.js",
  "directories": {
    "test": "tests"
  },
  "dependencies": {
    "elm": "^0.19.1-3",
    "firebase": "^6.6.2",
    "firebase-app": "0.0.1"
  },
  "devDependencies": {
    "chokidar-cli": "^2.1.0",
    "create-elm-app": "^4.2.26"
  },
  "scripts": {
    "build": "elm-app build",
    "start": "elm-app start",
    "where": "which elm",
    "watch": "chokidar '**/*.elm' -c 'elm make src/Main.elm --output main.js' --initial"
  },
  "keywords": [
    "elm",
    "firebase",
    "ports",
    "auth"
  ],
  "author": "Nigel Thorne",
  "license": "MIT"
}
